{
  "name": "builderhub",
  "version": "0.0.4",
  "description": "Builderhub CLI",
  "main": "dist/index.js",
  "bin": {
    "builderhub": "dist/index.js"
  },
  "scripts": {
    "dev": "dotenv -e .env.example ts-node-dev src/index.ts",
    "build": "tsc && tsc-alias",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "release": "standard-version --no-verify"
  },
  "keywords": [
    "cli",
    "builderhub",
    "chang soft I&I"
  ],
  "author": {
    "name": "Eunchurn Park",
    "email": "eunchurn.park@gmail.com"
  },
  "license": "MIT",
  "lint-staged": {
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@builderhub/eslint-config": "^0.0.6",
    "@builderhub/prettier-config": "^0.0.10",
    "@types/jest": "^29.5.3",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.4.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "husky": "8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "13.2.3",
    "pretty-quick": "3.1.3",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@clack/prompts": "^0.6.3",
    "picocolors": "^1.0.0"
  },
  "prettier": "@builderhub/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/builderhub-platform/builderhub.git"
  },
  "bugs": {
    "url": "https://github.com/builderhub-platform/builderhub/issues"
  },
  "homepage": "https://github.com/builderhub-platform/builderhub#readme"
}
